  "main": "index",
  "homepage": "https://github.com/Matt-Esch/string-template",
  "contributors": [
    {
      "name": "Matt-Esch"
    }
  ],
  "bugs": {
    "url": "https://github.com/Matt-Esch/string-template/issues",
    "email": "matt@mattesch.info"
  },
  "dependencies": {},
  "devDependencies": {
