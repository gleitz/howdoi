{
  "name": "string-template",
  "version": "0.2.1",
  "description": "A simple string template function based on named or indexed arguments",
  "keywords": [
    "template",
    "string",
    "format",
    "replace",
    "arguments"
  ],
  "author": "Matt-Esch <matt@mattesch.info>",
